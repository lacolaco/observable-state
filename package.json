{
  "name": "@lacolaco/reactive-store",
  "description": "A minimal implementation of state management with RxJS",
  "version": "3.0.2",
  "license": "MIT",
  "repository": "https://github.com/lacolaco/reactive-store.git",
  "private": false,
  "scripts": {
    "test": "jest --watch",
    "test:ci": "jest",
    "build": "run-p 'tsc:*'",
    "tsc:main": "tsc -p tsconfig.json",
    "tsc:esm5": "tsc -p tsconfig.esm5.json",
    "tsc:esm2015": "tsc -p tsconfig.esm2015.json",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "tslint -p tslint.json && prettier --check 'src/**/*.ts'",
    "release": "standard-version",
    "prepublishOnly": "yarn build"
  },
  "main": "dist/index.js",
  "module": "esm5/index.js",
  "es2015": "esm2015/index.js",
  "typings": "dist/index.d.ts",
  "peerDependencies": {
    "rxjs": "^6.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/jest": "24.0.13",
    "@types/node": "9.6.49",
    "immer": "3.1.2",
    "jest": "24.8.0",
    "npm-run-all": "4.1.5",
    "prettier": "1.18.2",
    "rimraf": "2.6.3",
    "rxjs": "6.5.2",
    "standard-version": "6.0.1",
    "ts-jest": "24.0.2",
    "tslint": "5.17.0",
    "typescript": "3.5.1"
  }
}
